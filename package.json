{
  "$schema": "https://json.schemastore.org/package",
  "name": "project-starter",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.18.0"
  },
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "postinstall": "pnpm run prisma:generate",
    "docker-compose:up": "docker compose --env-file ./.env up -d",
    "docker-compose:down": "docker compose --env-file ./.env down -v",
    "intl:generate": "pnpm run intl:extract && pnpm run intl:compile",
    "intl:extract": "pnpm run intl:extract:hr && pnpm run intl:extract:de && pnpm run intl:extract:es && pnpm run intl:extract:fr && pnpm run intl:extract:it && pnpm run intl:extract:en",
    "intl:extract:hr": "formatjs extract ./app/**/*.tsx --out-file ./app/core/intl/messages/hr/extracted.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl:extract:de": "formatjs extract ./app/**/*.tsx --out-file ./app/core/intl/messages/de/extracted.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl:extract:es": "formatjs extract ./app/**/*.tsx --out-file ./app/core/intl/messages/es/extracted.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl:extract:fr": "formatjs extract ./app/**/*.tsx --out-file ./app/core/intl/messages/fr/extracted.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl:extract:it": "formatjs extract ./app/**/*.tsx --out-file ./app/core/intl/messages/it/extracted.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl:extract:en": "formatjs extract ./app/**/*.tsx --out-file ./app/core/intl/messages/en/extracted.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl:compile": "pnpm run intl:compile:hr && pnpm run intl:compile:de && pnpm run intl:compile:es && pnpm run intl:compile:fr && pnpm run intl:compile:it && pnpm run intl:compile:en",
    "intl:compile:hr": "formatjs compile ./app/core/intl/messages/hr/extracted.json --ast --out-file ./app/core/intl/messages/hr/compiled.json",
    "intl:compile:de": "formatjs compile ./app/core/intl/messages/de/extracted.json --ast --out-file ./app/core/intl/messages/de/compiled.json",
    "intl:compile:es": "formatjs compile ./app/core/intl/messages/es/extracted.json --ast --out-file ./app/core/intl/messages/es/compiled.json",
    "intl:compile:fr": "formatjs compile ./app/core/intl/messages/fr/extracted.json --ast --out-file ./app/core/intl/messages/fr/compiled.json",
    "intl:compile:it": "formatjs compile ./app/core/intl/messages/it/extracted.json --ast --out-file ./app/core/intl/messages/it/compiled.json",
    "intl:compile:en": "formatjs compile ./app/core/intl/messages/en/extracted.json --ast --out-file ./app/core/intl/messages/en/compiled.json",
    "dev": "cross-env NODE_ENV=development react-router dev",
    "debug": "cross-env NODE_ENV=debug react-router dev",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest watch",
    "test:coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "build": "cross-env NODE_ENV=production react-router build",
    "serve": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "prisma:version": "prisma version",
    "prisma:init": "prisma init --schema ./prisma/schema",
    "prisma:generate": "prisma generate --schema ./prisma/schema",
    "prisma:validate": "prisma validate --schema ./prisma/schema",
    "prisma:format": "prisma format --schema ./prisma/schema",
    "prisma:debug": "prisma debug --schema ./prisma/schema",
    "prisma:studio": "prisma studio --schema ./prisma/schema",
    "prisma:db:pull": "prisma db pull --schema ./prisma/schema",
    "prisma:db:push": "prisma db push --schema ./prisma/schema",
    "prisma:migrate:dev": "prisma migrate dev --schema ./prisma/schema",
    "prisma:migrate:reset": "prisma migrate reset --schema ./prisma/schema",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ./prisma/schema",
    "prisma:migrate:resolve": "prisma migrate resolve --schema ./prisma/schema",
    "prisma:migrate:status": "prisma migrate status --schema ./prisma/schema",
    "prisma:migrate:diff": "prisma migrate diff --schema ./prisma/schema",
    "prisma:seed": "tsx ./prisma/scripts/seed/index.ts",
    "prisma:fake": "tsx ./prisma/scripts/fake/index.ts",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix .",
    "typecheck": "react-router typegen && tsc --noEmit",
    "clean": "rimraf ./.react-router ./build ./coverage ./node_modules"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@mantine/charts": "^8.2.7",
    "@mantine/core": "^8.2.7",
    "@mantine/dates": "^8.2.7",
    "@mantine/dropzone": "^8.2.7",
    "@mantine/hooks": "^8.2.7",
    "@mantine/modals": "^8.2.7",
    "@mantine/notifications": "^8.2.7",
    "@mantine/nprogress": "^8.2.7",
    "@mantine/spotlight": "^8.2.7",
    "@prisma/client": "^6.14.0",
    "@react-router/node": "^7.8.2",
    "@react-router/serve": "^7.8.2",
    "@tanstack/react-form": "^1.19.2",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@trpc/client": "^11.5.0",
    "@trpc/server": "^11.5.0",
    "@trpc/tanstack-react-query": "^11.5.0",
    "better-auth": "^1.3.7",
    "cross-env": "^10.0.0",
    "dayjs": "^1.11.13",
    "isbot": "^5.1.30",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-intl": "^7.1.11",
    "react-router": "^7.8.2",
    "recharts": "^3.1.2",
    "superjson": "^2.2.2",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@faker-js/faker": "^10.0.0",
    "@formatjs/cli": "^6.7.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@react-router/dev": "^7.8.2",
    "@react-router/fs-routes": "^7.8.2",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitest/coverage-v8": "3.2.4",
    "consola": "^3.4.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-formatjs": "^5.4.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
